canvas { 
	material: Terrain/TerrainColored;
	water: Water/TerrainWater;
	
	layer_canvas:            							gradient(#f5f5f5, #dcdcdc 5%, #cococo 10%, #a9a9a9 70%, #808080);
	layer_water:             							gradient(#87cefa, #1e90ff);
	layer_cars:											gradient(#4d4d4d, #404040);
	layer_pedestrian:									gradient(#636363, #4d4d4d);
}
/* flat areas, general rules */
area[amenity],
area[natural],
area[historic=memorial],
area[landuse]											{ builder: splat; fill-color: gradient(#10ff00, #0ff000 50%, #098a00); }

/* buildings */
area[man_made=tower],
area[building:levels],
area[building:shape],
area[building:part],
area[building]											{ builder: building; behaviour: solid; height:12; min_height: 0; 
														  facade-color:gradient(#22b5b5, #0f4f4f 50%, #000000); facade-builder:flat; facade-material:Materials/Buildings/Building;
														  roof-color:gradient(#15c0c0, #0a5a5a 50%, #000000); roof-builder:flat; roof-material:Materials/Buildings/Building; }

area[amenity=shelter][shelter_type=public_transport]		{ builder: building; behaviour: solid; height:3; min_height: 0;
															facade-color:gradient(#22b5b5, #0f4f4f 50%, #000000); facade-builder:flat; facade-material:Materials/Buildings/Building;
															roof-color:gradient(#15c0c0, #0a5a5a 50%, #000000); roof-builder:flat; roof-material:Materials/Buildings/Building; }

area[height]												{ height: eval(num(tag('height'))); }
area[building:height]									{ height: eval(num(tag('building:height'))); }
area[building:height][roof:height]						{ height: eval(num(tag('building:height')) - num(tag('roof:height'))); }

area[building:min_height]								{ min_height: eval(num(tag('building:min_height'))); }
area[min_height]											{ min_height: eval(num(tag('min_height'))); }

area[building=residential], 	
area[building:part],area[building=yes]					{ part: true; }	

area[building:shape=sphere]								{ builder: sphere; }
