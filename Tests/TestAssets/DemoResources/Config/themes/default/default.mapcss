way[highway=trunk],
way[highway=trunk_link],
way[highway=primary],
way[highway=primary_link],
way[highway=motorway],
way[highway=motorway_link]								{ fill-color: gradient(#18d2f9, #0e7c93 50%, #04262d); width:11; type:car; }


			
way[highway=secondary],
way[highway=secondary_link]								{ fill-color: gradient(#0e4eff, #0a37b4 50%, #04184e); width:9; type:car; }

way[highway=tertiary],
way[highway=tertiary_link]								{ fill-color: gradient(#18d2f9, #0e7c93 50%, #04262d); width:11; type:car; }


way[highway=path][foot=designated]						{ fill-color: gradient(#17ff0b, #0fa807 50%, #064203); width:3.5; type:pedestrian; }

way[highway=service]									{ fill-color: gradient(#0fffff, #0fffff 50%, #099999); width:3; type:pedestrian; }

way[highway=footway] 									{ width:2; type:pedestrian; }

way[barrier]											{ builder: barrier; material: Roads/Road; texture:Roads/Road; height:2; width:0.2;  }


