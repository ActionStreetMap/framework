/* roads */
way[highway]											{ builder:road; width:3; type:pedestrian; }

way[highway=tertiary],
way[highway=residential],
way[highway=service],
way[highway=unclassified],
way[highway=road]										{ fill-color: gradient(#0fffff, #0bbbbb 50%, #055555); width:12; type:car; }
