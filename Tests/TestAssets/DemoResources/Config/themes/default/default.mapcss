/************************************************** WAYS ********************************************/
way[waterway],
way[waterway][name]										{ fill-color: gradient(#1a9fa4, #0a3c3e 50%, #000000); builder: water; material: Water/Daylight_Simple_Water; behaviour: water; layer:4;}
														
