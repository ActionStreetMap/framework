<?xml version="1.0" encoding="utf-8" ?>
<settings environment="test">
  <scene>
	<!-- mapcss provider. Mapcss defines which elements are processed and what to use for them -->
	<stylesheet type="Mercraft.Core.MapCss.StylesheetProvider, Mercraft.Core">
		<path>Config\themes\default\default.mapcss</path>
	</stylesheet>
	<builders>
		<builder name="building" type="Mercraft.Explorer.Builders.BuildingModelBuilder, Mercraft.Explorer" />
		<builder name="solid" type="Mercraft.Explorer.Builders.SolidModelBuilder, Mercraft.Explorer"/>
		<builder name="flat" type="Mercraft.Explorer.Builders.FlatModelBuilder, Mercraft.Explorer"/>
		<builder name="sphere" type="Mercraft.Explorer.Builders.SphereModelBuilder, Mercraft.Explorer"/>
		<builder name="cylinder" type="Mercraft.Explorer.Builders.CylinderModelBuilder, Mercraft.Explorer"/>
	</builders>
	<behaviours>
		<behaviour name="solid" type="Mercraft.Explorer.Interactions.CompositeModelBehaviour, Mercraft.Explorer">
			<scripts>
				<include type="Assets.Scripts.Map.DestroyableObject, Assembly-CSharp" />
				<include type="Assets.Scripts.Map.LocationInfoHolder, Assembly-CSharp" />
			</scripts>
		</behaviour>
	</behaviours>
	<world>
		<buildings>
			<builder type="Mercraft.Models.Buildings.BuildingBuilder, Mercraft.Models" />
			<style type="Mercraft.Explorer.Themes.BuildingStyleProvider, Mercraft.Explorer" />
		</buildings>
	</world>
  </scene>
</settings>